{
  "env": {
    "commonjs": true,
    "es2021": true,
    "node": true
  },
  "globals": {
    "awslambda": "readonly"
  },
  "extends": ["airbnb-base"],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
    "import/extensions": [
      "error",
      {
        "js": "ignorePackages"
      }
    ],
    "semi": ["error", "always"]
  }
}
